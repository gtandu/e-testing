<mapping>
	<class name="fr.etesting.etesting.model.Qcm" container="true">
		<map-to xml="qcm" />
		<field name="listeQuestionsReponses" type="fr.etesting.etesting.model.QuestionReponse" collection="arraylist" container="false">
			<bind-xml name="listeQuestionsReponses" node="element" />
		</field>
	</class>
	<class name="fr.etesting.etesting.model.QuestionReponse">
		<map-to xml="questionReponse" />
		<field name="libelleQuestion" type="java.lang.String">
			<bind-xml name="libelleQuestion" node="element" />
		</field>
		<field name="listeReponsesCorrectes" type="fr.etesting.etesting.model.ReponseCorrecte" collection="arraylist" container="false">
			<bind-xml name="listeReponsesCorrectes" node="element" />
		</field>
		<field name="listeReponsesFausses" type="fr.etesting.etesting.model.ReponseFausse" collection="arraylist" container="false">
			<bind-xml name="listeReponsesFausses" node="element" />
		</field>
	</class>
	<class name="fr.etesting.etesting.model.ReponseCorrecte" container="false">
		<map-to xml="reponseCorrecte" />
		<field name="libelleReponse" type="java.lang.String">
			<bind-xml name="libelleReponse" node="element" />
		</field>
	</class>
	<class name="fr.etesting.etesting.model.ReponseFausse" container="false">
		<map-to xml="reponseFausse" />
		<field name="libelleReponse" type="java.lang.String">
			<bind-xml name="libelleReponse" node="element" />
		</field>
	</class>
</mapping>